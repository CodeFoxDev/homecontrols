{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "ES6",
    "module": "ES6",
    "paths": {
      "#api/*": [
        "../api/*"
      ],
      "#builtin/*": [
        "../builtin/*"
      ],
      "types/*": [
        "../api/types/*.d.ts"
      ],
      "*": [
        "../api/types/*.d.ts"
      ]
    },
    "allowJs": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}