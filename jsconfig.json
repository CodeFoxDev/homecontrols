{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES6",
    "module": "ES6",
    "paths": {
      "#src/*": [
        "src/*"
      ],
      "#builtin/*": [
        "builtin/*"
      ],
      "types/*": [
        "./src/types/*.d.ts"
      ],
      "*": [
        "./src/types/*.d.ts"
      ]
    },
    "allowJs": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}